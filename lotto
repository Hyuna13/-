import random

lotto = []
while True:
    temp = random.random()
    temp = temp * 100
    if temp >= 1 and temp <46:
        lotto.append(int(temp))
        lotto.set(lotto) #중복방지
        lotto.list(lotto)
        
    if len(lotto) == 6:
        break
print(lotto)

or 모듈함수사용
#randint
#sample
print(random.randint(1,45))
print(random.sample(range(1,45),6))
