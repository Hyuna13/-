# 빈 리스트 만들기
numbers = []
print(numbers)

# numbers에 값들 추가
numbers.extend([1, 7, 3, 6, 5, 2, 13, 14])  
print(numbers)

or

numbers.append(1)
numbers.append(7)
numbers.append(3)
numbers.append(6)
numbers.append(5)
numbers.append(2)
numbers.append(13)
numbers.append(14)
print(numbers)

#홀수 모두 제거
i = 0
while i < len(numbers):
    if numbers[i] % 2 == 1:
       del numbers[i]
    i += 1
print(numbers)

#인덱스 0 자리에 20을 삽입
numbers.insert(0, 20)
print(numbers)

#정렬
numbers.sort()
print(numbers)
